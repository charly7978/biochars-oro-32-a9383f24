
{
  "runtime": "incremental",
  "resources": [],
  "use": ["@assemblyscript/transform-wasm-simd"],
  "entries": [
    "./assembly/signal-processor.ts"
  ],
  "options": {
    "transform": ["@assemblyscript/transform-wasm-simd"],
    "importMemory": true,
    "initialMemory": 2,
    "maximumMemory": 16,
    "sharedMemory": true,
    "exportMemory": true,
    "memoryBase": 0
  },
  "configurations": {
    "debug": {
      "debug": true,
      "sourceMap": true
    },
    "release": {
      "optimizeLevel": 3,
      "shrinkLevel": 2,
      "converge": false,
      "noAssert": true,
      "bindings": "esm"
    }
  }
}
